@model CvProjem.Models.Entity.TblYeteneklerim

@{
    ViewBag.Title = "YetenekDuzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2> Yetenek Düzenleme Sayfası</h2>
<br />
<form method="post">


    @Html.Label("Yetenek Açıklaması")
    @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
    <br />

    <br />
    @Html.Label("Derece")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })
    <br />


    <button class="btn btn-succes">Güncelle</button>

</form>


